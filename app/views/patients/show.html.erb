<table class="table table-hover">
	<tr>
		<th>First Name</th>
		<th>Last Name</th>
		<th>DOB</th>
		<th>Gender</th>
		<th>Discharge Date</th>
	</tr>
	<% @patients.each do |patient| %>
		<tr>
			<td><%= patient.first_name %></td>
			<td><%= patient.last_name %></td>
			<td><%= patient.birthday %></td>
			<td><%= patient.gender %></td>
			<td><%= patient.discharge_date %></td>
		</tr>
	<% end %>
</table>